# Database configuration
# These variables are used for database connection
db_host=localhost     # Database host address
db_port=9090         # Database port number
db_un=rtmphm         # Database username
db_pw=rtmphm         # Database password

# Vimeo API configuration
# These variables are required for Vimeo API authentication
VIMEO_EVENT_ID=      # Your Vimeo live event ID (e.g., '1231231')
VIMEO_TOKEN=         # Your Vimeo API access token
VIMEO_KEY=           # Your Vimeo API key/client identifier
VIMEO_SECRET=        # Your Vimeo API secret

# Note: Copy this file to .env and fill in your actual values
# Do not commit the .env file to version control
